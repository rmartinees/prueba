<!DOCTYPE html>
<html>
<head>
  <%- include ("../partials/header.ejs") %>
</head>

<body>

<%- include ("../partials/nav.ejs") %>

<div class="container">
<h2>Temperatura en el campo de <span id=Campo></span></h2>


<table>
  <tr>
    <th>Fecha</th>
    <th>Viento</th>
    <th>MaxViento</th>
    <th>DirViento</th>
    <th>Temperatura</th>
  </tr>
    <% results.forEach(function(r) { %>
        <tr><td> <%= r.fecha %>  </td>  <td> <%= r.veloviento %> </td> <td>- <%= r.maxviento %></td><td> <%= r.dirviento %> </td><td> <%= r.temp %></td></tr>
    <% }); %>
</table>

</div>

</body>
</html>
